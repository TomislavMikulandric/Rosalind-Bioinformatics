   "source": [
    "def hamming_distance(s1,s2):\n",
    "    return sum(c1 != c2 for c1, c2 in zip(s1,s2))\n",
    "\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    with open(\"../data/rosalind_ba1g.txt\") as f:\n",
    "        dna1 = f.readline().strip()\n",
    "        dna2 = f.readline().strip()\n",
    "    dist = hamming_distance(dna1, dna2)\n",
    "    print(dist)"
